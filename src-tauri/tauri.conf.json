{
	"build": {
		"beforeDevCommand": "make start-development-frontend",
		"beforeBuildCommand": "make build-production-frontend",
		"devPath": "http://localhost:5000",
		"distDir": "../build"
	},
	"package": {
		"productName": "Update NPM Packages",
		"version": "0.0.0"
	},
	"tauri": {
		"windows": [
			{
				"title": "Update NPM Packages",
				"maximized": true
			}
		],
		"security": {
			"csp": null
		},
		"bundle": {
			"active": true,
			"targets": "all",
			"identifier": "com.npm.packages",
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			]
		}
	}
}
